        (pair %start
           (list %assets_offered
              (pair (address %fa2_address) (list %tokens (pair (nat %token_id) (nat %amount)))))
           (list %assets_requested
              (pair (address %fa2_address) (list %tokens (pair (nat %token_id) (nat %amount))))))) ;
