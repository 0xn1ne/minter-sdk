                 IF_NONE { FAILWITH } { SWAP ; DROP } ;
                 SWAP ;
                 CDR ;
                 MAP { DUP 4 ; PAIR } ;
                 DIG 3 ;
                 DROP ;
                 DIG 2 ;
                 PAIR ;
                 SWAP ;
                 PUSH mutez 0 ;
                 NIL (pair address (list (pair address (pair nat nat)))) ;
                 DIG 3 ;
                 CONS ;
                 TRANSFER_TOKENS } ;
             SWAP ;
             APPLY } ;
         LAMBDA
           (pair (pair (list (pair address (list (pair nat nat)))) (list (pair address (list (pair nat nat)))))
                 (pair address (or unit (or address unit))))
           unit
           { CDR ;
             CDR ;
             IF_LEFT
               { DROP ; PUSH unit Unit }
               { IF_LEFT
                   { DROP ; PUSH string "SWAP_FINISHED" ; FAILWITH }
                   { DROP ; PUSH string "SWAP_CANCELLED" ; FAILWITH } } } ;
         DIG 2 ;
         UNPAIR ;
         IF_LEFT
           { IF_LEFT
               { SWAP ;
                 DUP ;
                 DUG 2 ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 SWAP ;
                 CDR ;
                 SWAP ;
                 GET ;
                 IF_NONE { PUSH string "SWAP_NOT_EXIST" ; FAILWITH } {} ;
                 DUP ;
                 DIG 4 ;
                 SWAP ;
                 EXEC ;
                 DROP ;
                 SENDER ;
                 LEFT unit ;
                 RIGHT unit ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 CDR ;
                 CAR ;
                 PAIR ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 CAR ;
                 PAIR ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 CAR ;
                 CAR ;
                 MAP { PUSH string "SWAP_OFFERED_FA2_INVALID" ;
                       SENDER ;
                       SELF_ADDRESS ;
                       PAIR ;
                       PAIR ;
                       DUP 7 ;
                       SWAP ;
                       EXEC ;
                       SWAP ;
                       EXEC } ;
                 DUP 3 ;
                 CAR ;
                 CDR ;
                 MAP { PUSH string "SWAP_REQUESTED_FA2_INVALID" ;
                       DUP 5 ;
                       CDR ;
                       CAR ;
                       SENDER ;
                       PAIR ;
                       PAIR ;
                       DUP 8 ;
                       SWAP ;
                       EXEC ;
                       SWAP ;
                       EXEC } ;
                 DIG 3 ;
